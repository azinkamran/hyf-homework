{
  "name": "meal-sharing",
  "version": "1.0.0",
  "description": "Website for sharing meals",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "start": "npm run build && node ./src/backend/index",
    "build": "webpack --mode production",
    "server": "nodemon ./src/backend/index.js",
    "dev": "concurrently \"npm run server\"",
    "reinstall": "npm run delete-nm && rm ./package-lock.json && npm run delete-nm && npm install",
    "delete-nm": "rm -rf node_modules/",
    "delete-pl": "rm ./package-lock.json"
  },
  "author": "Benjamin Hughes",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@kodnificent/sparouter": "^1.1.0",
    "@types/xml": "^1.0.3",
    "app-root-path": "^2.1.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.0",
    "disqus-react": "^1.0.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "express-brute": "^1.0.1",
    "express-form-data": "^2.0.1",
    "multer": "^1.3.0",
    "mysql": "^2.17.1",
    "path": "^0.12.7",
    "serve-favicon": "2.4.5",
    "swagger-ui-express": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.12.1",
    "sequelize-cli": "^5.4.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.0"
  }
}
